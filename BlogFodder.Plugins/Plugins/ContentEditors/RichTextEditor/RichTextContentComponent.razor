@using System.Text.Json
@ContentModel.Testing

@code {
    [Parameter] public string? Model { get; set; }
    private RichTextContentModel ContentModel { get; set; } = new();
    protected override void OnInitialized()
    {
        if (Model != null)
        {
            ContentModel = JsonSerializer.Deserialize<RichTextContentModel>(Model) ?? new RichTextContentModel();
        }
    }

}